{
    "name": "chand335/schema-sync",
    "description": "Laravel package to compare database schemas and generate synchronization migrations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chand335"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Chand335\\SchemaSync\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chand335\\SchemaSync\\SchemaSyncServiceProvider"
            ]
        }
    }
}
